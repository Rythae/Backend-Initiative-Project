{
  "name": "backend-initiative-project",
  "version": "1.0.0",
  "description": "This is a base repository for the backend initiative project",
  "main": "index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development npm run migrate && cross-env NODE_ENV=development nodemon server.js",
    "migrate": "node db/migration.js",
    "dev": "node db/index.js",
    "start": "nodemon server.js",
    "db:ready": "npm-run-all db:undo db:migrate db:seed",
    "db:migrate": "sequelize db:migrate",
    "db:undo": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rythae/Backend-Initiative-Project.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rythae/Backend-Initiative-Project/issues"
  },
  "homepage": "https://github.com/Rythae/Backend-Initiative-Project#readme",
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "@sendgrid/mail": "^7.4.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^5.1.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
